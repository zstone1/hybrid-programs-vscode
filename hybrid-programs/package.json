{
	"name": "hybrid-programs",
	"displayName": "Hybrid Programs",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.hybrid-programs"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "hybrid-program",
				"extensions": [".hp"]
			}
		],
		"grammars": [
			{
				"language": "hybrid-program",
				"scopeName": "source.hybrid-program",
				"path": "./syntaxes/hp.tmLanguage.json"
			}
		],
		"commands": [
			{
				"command": "extension.hybrid-programs",
				"title": "Sup Nerds"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
